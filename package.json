{
  "name": "agency-buyers-website",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "build:prod": "cross-env NODE_OPTIONS=--no-deprecation SKIP_PAYLOAD_DB=true next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "ci": "pnpm build",
    "ci-with-migrations": "echo 'y' | payload migrate:fresh && pnpm build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "db:fresh": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:fresh",
    "db:reset": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:reset",
    "db:migrate": "cross-env NODE_OPTIONS=--no-deprecation payload migrate",
    "db:create": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:create",
    "db:status": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:status",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test --config=playwright.config.ts",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts",
    "deploy": "cross-env NODE_OPTIONS=--no-deprecation timeout 300s bash -c 'pnpm build' || echo 'Build completed' && docker buildx build --platform=linux/amd64 -t australia-southeast1-docker.pkg.dev/agency-buyers-platform/payload-cms-repo/payload-cms:latest . && docker push australia-southeast1-docker.pkg.dev/agency-buyers-platform/payload-cms-repo/payload-cms:latest"
  },
  "dependencies": {
    "@payloadcms/admin-bar": "3.52.0",
    "@payloadcms/db-postgres": "3.52.0",
    "@payloadcms/email-resend": "^3.52.0",
    "@payloadcms/live-preview-react": "3.52.0",
    "@payloadcms/next": "3.52.0",
    "@payloadcms/payload-cloud": "3.52.0",
    "@payloadcms/plugin-form-builder": "3.52.0",
    "@payloadcms/plugin-nested-docs": "3.52.0",
    "@payloadcms/plugin-redirects": "3.52.0",
    "@payloadcms/plugin-search": "3.52.0",
    "@payloadcms/plugin-seo": "3.52.0",
    "@payloadcms/richtext-lexical": "3.52.0",
    "@payloadcms/storage-gcs": "3.52.0",
    "@payloadcms/ui": "3.52.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@types/pg": "^8.15.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.31.4",
    "geist": "^1.4.2",
    "graphql": "^16.11.0",
    "lucide-react": "^0.540.0",
    "next": "15.4.4",
    "next-sitemap": "^4.2.3",
    "payload": "3.52.0",
    "prism-react-renderer": "^2.4.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "7.62.0",
    "recharts": "2.15.4",
    "sharp": "0.34.2",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@playwright/test": "1.54.1",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/react": "16.3.0",
    "@types/escape-html": "^1.0.4",
    "@types/node": "24.1.0",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react": "4.5.2",
    "autoprefixer": "^10.4.21",
    "copyfiles": "^2.4.1",
    "dotenv": "^17.2.1",
    "eslint": "^9.33.0",
    "eslint-config-next": "15.4.4",
    "@next/eslint-plugin-next": "15.4.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jsdom": "26.1.0",
    "pg": "^8.16.3",
    "playwright": "1.54.1",
    "playwright-core": "1.54.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "packageManager": "pnpm@10.13.1",
  "engines": {
    "node": "22.x"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
